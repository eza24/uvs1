{% extends 'base.html.twig'%}
{% block title %} {{ article.title }} {% endblock %}

{% block body %}
{{ render(controller(
    'App\\Controller\\HomeController::index'
)) }}
</div>
<div class="main-content">
    <div class="container-fluid">
        <div class="page-header">
            <div class="row align-items-end">
                <div class="col-lg-8">
                    <div class="page-header-title">
                        <i class="ik ik-eye theme-color"></i>
                        <div class="d-inline">
                            <h5> {{ article.title }}</h5>
                            <span></span>
                        </div>
                    </div>
                </div>

            </div>
        </div>

        <div class="row layout-wrap" id="layout-wrap">
            <div class="col-md-12">
                <div class="card" id="card1">
                    <div class="card-header" style="flex-flow: row-reverse;">
                    </div>
                    <div class="card-body">
                        <table class="table">
                            <tbody>
                            <tr>
                                <th>Id</th>
                                <td>{{ interview.id }}</td>
                            </tr>
                            <tr>
                                <th>Title</th>
                                <td>{{ interview.title }}</td>
                            </tr>
                            <tr>
                                <th>Bodys</th>
                                <td>{{ interview.bodys }}</td>
                            </tr>
                            <tr>
                                <th>Picture</th>
                                <td>{{ interview.picture }}</td>
                            </tr>
                            <tr>
                                <th>Types</th>
                                <td>{{ interview.types }}</td>
                            </tr>
                            <tr>
                                <th>Status</th>
                                <td>{{ interview.status }}</td>
                            </tr>
                            <tr>
                                <th>Views</th>
                                <td>{{ interview.views }}</td>
                            </tr>
                            <tr>
                                <th>Date</th>
                                <td>{{ interview.date ? interview.date|date('Y-m-d') : '' }}</td>
                            </tr>
                            <tr>
                                <th>Comments</th>
                                <td>{{ interview.comments }}</td>
                            </tr>
                            <tr>
                                <th>Sort</th>
                                <td>{{ interview.sort }}</td>
                            </tr>
                            <tr>
                                <th>Who</th>
                                <td>{{ interview.who }}</td>
                            </tr>
                            <tr>
                                <th>CreatedDate</th>
                                <td>{{ interview.createdDate ? interview.createdDate|date('Y-m-d H:i:s') : '' }}</td>
                            </tr>
                            </tbody>
                        </table>
                        {#                        <button type="submit" name="form_name[submit]" class="btn btn-primary">Хадгалах</button>#}

                        <button onclick="history.back()" class="btn btn-light">Буцах</button>
                        {#                        <a href="{{ path('article_index') }}">back to list</a>#}

                        {#                        <a href="{{ path('article_edit', {'id': article.id}) }}">edit</a>#}

                        {{ include('article/_delete_form.html.twig') }}
                    </div>
                </div>
            </div>
        </div>
        {% endblock %}



